{
  "extends": [
    "tslint-stencil/default",
    "tslint-ionic-rules",
    "tslint-config-prettier" // <- required to be last to disable conflicting rules
  ],
  "rules": {
    "component-member-order": {
      "severity": "warn"
    },
    "decorated-member-style": {
      "severity": "off"
    }
  }
}
