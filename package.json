{
  "name": "@esri/calcite-app-components",
  "version": "4.11.1",
  "description": "A collection of web components for building single-page applications.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "jsnext:main": "dist/esm/index.mjs",
  "unpkg": "dist/calcite-app.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "build:docs": "stencil build --config stencil.storybook.config.ts",
    "demoPageSync": "ts-node ./support/demoPageSync.ts",
    "deps:update": "updtr --ex @types/jest @types/puppeteer jest jest-cli puppeteer && git add package*.json && git commit -q -m \"chore(deps): Bump versions.\"",
    "docs": "concurrently --kill-others --raw \"npm run build:docs && build-storybook --static-dir ./__docs-temp__ --output-dir ./docs\" \"ts-node ./support/tempDocDirCleaner.ts\"",
    "docs:preview": "concurrently --raw \"npm run build:docs && start-storybook --static-dir ./__docs-temp__\" \"ts-node ./support/tempDocDirCleaner.ts\"",
    "lint": "concurrently npm:lint:*",
    "lint:ts": "tslint --project tsconfig-tslint.json",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "prepublishOnly": "ts-node ./support/prepublish.ts",
    "prettier": "concurrently npm:prettier:*",
    "prettier:ts": "prettier --write \"src/**/*.ts?(x)\"",
    "prettier:styles": "prettier --write \"src/**/*.scss\"",
    "preversion": "npm ci && npm test",
    "release": "git push --follow-tags && npm publish && ts-node ./support/githubReleaseFromChangelog.ts",
    "start": "concurrently --kill-others --raw \"tsc --project ./tsconfig-demos.json --watch\" \"npm run demoPageSync\" \"stencil build --dev --watch --serve\"",
    "test": "stencil test --spec --e2e",
    "test:headed": "stencil test --spec --e2e --devtools",
    "test:watch": "stencil test --spec --e2e --watch",
    "test:watchAll": "stencil test --spec --e2e --watchAll",
    "version": "npm run build && npm run docs && standard-version && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/calcite-app-components.git"
  },
  "dependencies": {
    "@esri/calcite-base": "1.2.0",
    "@esri/calcite-colors": "1.6.0",
    "@esri/calcite-ui-icons": "2.7.1",
    "lodash-es": "4.17.15",
    "sortablejs": "1.10.1"
  },
  "devDependencies": {
    "@esri/calcite-components": "1.0.0-beta.15",
    "@stencil/core": "1.8.1",
    "@stencil/postcss": "1.0.1",
    "@stencil/sass": "1.1.1",
    "@stencil/state-tunnel": "1.0.1",
    "@storybook/addon-backgrounds": "5.2.8",
    "@storybook/addon-centered": "5.2.8",
    "@storybook/addon-docs": "5.2.8",
    "@storybook/addon-knobs": "5.2.8",
    "@storybook/addon-notes": "5.2.8",
    "@storybook/html": "5.2.8",
    "@types/autoprefixer": "9.6.1",
    "@types/jest": "24.0.21",
    "@types/jest-axe": "3.2.1",
    "@types/lodash-es": "4.17.3",
    "@types/puppeteer": "1.20.0",
    "@types/rimraf": "2.0.3",
    "@types/sortablejs": "1.10.1",
    "@types/webpack": "4.41.0",
    "autoprefixer": "9.7.3",
    "awesome-typescript-loader": "5.2.1",
    "axe-core": "3.4.1",
    "babel-loader": "8.0.6",
    "chalk": "3.0.0",
    "chokidar": "3.3.1",
    "classnames": "2.2.6",
    "concurrently": "5.0.2",
    "gh-release": "3.5.0",
    "github-release-from-cc-changelog": "2.2.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-axe": "3.2.0",
    "jest-cli": "24.9.0",
    "jest-runner": "24.9.0",
    "lint-staged": "9.5.0",
    "package": "1.0.1",
    "prettier": "1.19.1",
    "puppeteer": "1.20.0",
    "rimraf": "3.0.0",
    "standard-version": "7.0.1",
    "stylelint": "12.0.0",
    "stylelint-config-recommended-scss": "4.1.0",
    "stylelint-scss": "3.13.0",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-ionic-rules": "0.0.21",
    "tslint-lines-between-class-members": "1.3.6",
    "tslint-stencil": "1.0.1",
    "updtr": "3.1.0"
  },
  "peerDependencies": {
    "@esri/calcite-components": "1.0.0-beta.10"
  },
  "license": "Apache-2.0"
}
